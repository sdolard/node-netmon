			"enabled": true,
			"task": [
				{
					"enabled": true,
					"description": "Google.comm ping",
					"action": "ping", 
					"verbose": false,
					"config": {
						"host": "www.google.com"
					}
				},{
					"enabled": true,
					"description": "Google.fr ping",
					"action": "ping", 
					"verbose": false,
					"config": {
						"host": "www.google.fr"
					}
				}
			]
		}
	]
}
